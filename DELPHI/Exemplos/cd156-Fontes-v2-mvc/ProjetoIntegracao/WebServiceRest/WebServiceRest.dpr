  WebModuleUnit1 in 'WebModuleUnit1.pas' {WebModule1: TWebModule},
  UDataModule in 'UDataModule.pas' {FDataModule: TDataModule};

{$R *.res}

begin
  if WebRequestHandler <> nil then
    WebRequestHandler.WebModuleClass := WebModuleClass;
